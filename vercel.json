{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ],
    "buildCommand": "npm install --legacy-peer-deps && ng build --configuration=production",
    "outputDirectory": "dist/cafeteria-web/browser"
}